    "moment": "^2.29.1",
    "mongodb": "^4.1.2",
    "mqtt": "^4.3.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io-client": "^2.3.0",
    "socket.io-redis": "^6.1.1",
    "standard-version": "^9.3.2",
    "uuid": "^8.3.2"
